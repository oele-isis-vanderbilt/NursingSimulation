{
  "mode": "record",
  "runtime": 600,
  "workers": {
    "manager_ip": "129.59.104.153",
    "manager_port": 9001,
    "instances": [
      {
        "name": "local",
        "id": "local",
        "description": "local worker for the MMLA pipeline demo with a video node"
      }
    ]
  },
  "nodes": [
    {
      "registry_name": "NS_WRTC",
      "name": "screen-audio-capture",
      "kwargs": {
        "host": "localhost",
        "port": 8080,
        "video_chunk_key": "frame"
      },
      "package": "nursing-simulation"
    },
     {
      "registry_name": "ChimeraPyRerunNode",
      "name": "rerun-show",
      "kwargs": {
        "application_id": "chimerapy-rerun-node",
        "server_addr": "127.0.0.1:54000",
        "rerun_mappings": {
          "frame": {
            "primitive": true,
            "type": "Image"
          }
        }
      },
      "package": "chimerapy-rerun-node"
    }
  ],
  "adj": [
    [
      "screen-audio-capture",
      "rerun-show"
    ]
  ],
  "manager_config": {
    "logdir": "cp-logs",
    "port": 9001
  },
  "mappings": {
    "local": [
      "screen-audio-capture",
      "rerun-show"
    ]
  }
}
